# Application Configuration
VITE_APP_ID=your-app-id
VITE_APP_TITLE=Bottleneck Bots
VITE_APP_LOGO=/logo.png

# Authentication
JWT_SECRET=your-jwt-secret-key-here

# Database
DATABASE_URL=your-database-connection-string

# Redis Cache (for distributed rate limiting and caching)
REDIS_URL=redis://localhost:6379
REDIS_KEY_PREFIX=bb:

# OAuth Configuration
OAUTH_SERVER_URL=your-oauth-server-url
OWNER_OPEN_ID=your-owner-open-id

# Forge API (if using)
BUILT_IN_FORGE_API_URL=your-forge-api-url
BUILT_IN_FORGE_API_KEY=your-forge-api-key

# Analytics (optional)
VITE_ANALYTICS_ENDPOINT=your-analytics-endpoint
VITE_ANALYTICS_WEBSITE_ID=your-analytics-website-id

# Server Configuration
PORT=3000
NODE_ENV=production

# AI Model API Keys
GEMINI_API_KEY=your-gemini-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Browser Automation
BROWSERBASE_API_KEY=
BROWSERBASE_PROJECT_ID=
# Browserbase region (default: us-west-2)
BROWSERBASE_REGION=us-west-2

# Stagehand AI Model Configuration
# Options: google/gemini-2.0-flash, openai/gpt-4o, anthropic/claude-3-sonnet
STAGEHAND_MODEL=google/gemini-2.0-flash
# Alternative: use AI_MODEL as fallback
AI_MODEL=google/gemini-2.0-flash

# Set to 1 to skip downloading browsers (required for Vercel)
PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1

# GoHighLevel API Configuration (for ghl_action tasks)
GHL_API_KEY=your-gohighlevel-api-key
GHL_SECRET_KEY=your-gohighlevel-secret-key
GHL_LOCATION_ID=your-default-location-id

# Session cleanup interval in milliseconds (default: 5 minutes)
SESSION_CLEANUP_INTERVAL=300000

# AWS S3 Storage Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket-name

# CloudFront CDN Configuration (optional)
CDN_ENABLED=false
CLOUDFRONT_DISTRIBUTION_ID=your-distribution-id
CLOUDFRONT_DOMAIN=your-distribution.cloudfront.net
# For signed URLs (private content)
CLOUDFRONT_KEY_PAIR_ID=your-key-pair-id
# Note: For CLOUDFRONT_PRIVATE_KEY, use \n for newlines or store in a file
CLOUDFRONT_PRIVATE_KEY=

# Stripe Payment Integration
# Get your keys from: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
# Webhook secret from: https://dashboard.stripe.com/webhooks
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
# Application URL for Stripe redirects
APP_URL=http://localhost:3000

# Meta Ads Integration
# Get your app credentials from: https://developers.facebook.com/apps
META_APP_ID=your-meta-app-id
META_APP_SECRET=your-meta-app-secret
# OAuth redirect URI (must match your app settings)
META_REDIRECT_URI=http://localhost:3000/oauth/meta/callback

# Email Integration (Gmail OAuth)
# Get credentials from: https://console.cloud.google.com/apis/credentials
GMAIL_CLIENT_ID=your_gmail_client_id_here
GMAIL_CLIENT_SECRET=your_gmail_client_secret_here
GMAIL_REDIRECT_URI=http://localhost:3000/api/oauth/gmail/callback

# Email Integration (Outlook OAuth)
# Get credentials from: https://portal.azure.com/#view/Microsoft_AAD_RegisteredApps
OUTLOOK_CLIENT_ID=your_outlook_client_id_here
OUTLOOK_CLIENT_SECRET=your_outlook_client_secret_here
OUTLOOK_REDIRECT_URI=http://localhost:3000/api/oauth/outlook/callback

# Encryption Key for OAuth Tokens
# Generate with: openssl rand -hex 32
ENCRYPTION_KEY=your_64_character_hex_encryption_key_here

# Google OAuth Configuration (User Authentication)
# Get credentials from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://www.bottleneckbot.com/api/oauth/google/callback

# Sentry Error Tracking
# Get your DSN from: https://sentry.io/settings/[org]/projects/[project]/keys/
SENTRY_DSN=your_sentry_dsn_here
# For client-side tracking, also set this in your Vercel environment variables as VITE_SENTRY_DSN
VITE_SENTRY_DSN=your_sentry_dsn_here

# Email Configuration
# Resend API (preferred method)
RESEND_API_KEY=your-resend-api-key

# SMTP Configuration (fallback if Resend not available)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-email@example.com
SMTP_PASS=your-app-password

# Email Addresses
SUPPORT_EMAIL=support@bottleneckbot.com
ADMIN_NOTIFICATION_EMAIL=admin@bottleneckbot.com

# Slack Notifications
SLACK_WEBHOOK_URL=your-slack-webhook-url
SLACK_DEFAULT_CHANNEL=#support-tickets
